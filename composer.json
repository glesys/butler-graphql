{
    "authors": [
        {
            "name": "Christoffer Artmann",
            "email": "christoffer.artmann@glesys.se"
        },
        {
            "name": "Christoffer Persson",
            "email": "christoffer@glesys.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "Butler\\Graphql\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/stubs/"
        ],
        "psr-4": {
            "Butler\\Graphql\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "An opinionated GraphQL package for Laravel",
    "extra": {
        "laravel": [
            "Butler\\Graphql\\ServiceProvider"
        ]
    },
    "license": "MIT",
    "name": "glesys/butler-graphql",
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6",
        "leinonen/php-dataloader": "^1.0",
        "webonyx/graphql-php": "^0.11.5"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.1",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "type": "library"
}
