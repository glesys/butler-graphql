{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "no_multiline_whitespace_around_double_arrow": false,
        "class_attributes_separation" : {
            "elements" : {
                "method" : "one",
                "trait_import" : "none"
            }
        },
        "new_with_parentheses": {
            "anonymous_class": false,
            "named_class": true
        },
        "php_unit_method_casing": false
    }
}
